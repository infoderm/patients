    "@visx/scale": "^3.3.0",
    "@visx/shape": "^3.3.0",
    "@visx/tooltip": "^3.3.0",
    "bcrypt": "^5.1.1",
    "canvas": "^2.11.2",
    "chardet": "^1.6.0",
    "colord": "^2.9.3",
    "core-js": "^3.32.2",
    "date-fns": "^2.30.0",
    "debounce": "^1.2.1",
    "downshift": "^7.2.1",
    "email-addresses": "^5.0.0",
    "escape-string-regexp": "^5.0.0",
    "express": "^4.18.2",
    "fast-diff": "^1.3.0",
    "file-saver": "^2.0.5",
    "healthone": "^6.1.0",
    "history": "^5.3.0",
    "iban": "^0.0.14",
    "ical-generator": "^5.0.1",
    "iconv-lite": "^0.6.3",
    "idb": "^6.1.5",
