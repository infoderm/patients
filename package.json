{
  "name": "patients",
  "meteor": {
    "mainModule": {
      "client": "client/main.tsx",
      "server": "server/main.ts"
    }
  },
  "babel": {
    "plugins": [
      "@babel/plugin-syntax-jsx"
    ],
    "env": {
      "COVERAGE": {
        "sourceMaps": "both",
        "plugins": [
          [
            "transform-remove-console",
            {
              "exclude": [
                "time",
                "timeEnd",
                "log",
                "error",
                "warn"
              ]
            }
          ],
          "istanbul"
        ]
      }
    }
  },
  "dependencies": {
    "@async-iterable-iterator/async-iterable-to-array": "0.0.1",
    "@babel/runtime": "^7.18.3",
    "@combinatorics/n-combinations": "^0.1.1",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@functional-abstraction/operator": "^1.0.4",
    "@heap-data-structure/pairing-heap": "^3.0.1",
    "@iterable-iterator/cardinality": "^3.0.4",
    "@iterable-iterator/chain": "^1.0.0",
    "@iterable-iterator/filter": "^0.1.2",
    "@iterable-iterator/group": "^0.0.1",
    "@iterable-iterator/iter": "^0.0.2",
    "@iterable-iterator/list": "^0.0.2",
    "@iterable-iterator/map": "^0.1.0",
    "@iterable-iterator/next": "^1.0.1",
    "@iterable-iterator/range": "^1.0.0",
    "@iterable-iterator/reduce": "^0.0.1",
    "@iterable-iterator/slice": "^0.0.1",
    "@iterable-iterator/sorted": "^0.0.2",
    "@iterable-iterator/window": "^0.0.3",
    "@iterable-iterator/zip": "^0.0.2",
    "@mui/icons-material": "^5.8.0",
    "@mui/lab": "^5.0.0-alpha.83",
    "@mui/material": "^5.8.1",
    "@mui/styles": "^5.8.0",
    "@set-theory/cartesian-product": "^1.0.2",
    "@total-order/date": "^0.0.2",
    "@total-order/iter": "^0.0.2",
    "@total-order/key": "^0.0.1",
    "@total-order/primitive": "^0.0.2",
    "@visx/axis": "^2.10.0",
    "@visx/event": "^2.6.0",
    "@visx/gradient": "^2.10.0",
    "@visx/grid": "^2.10.0",
    "@visx/group": "^2.10.0",
    "@visx/legend": "^2.10.0",
    "@visx/mock-data": "^2.1.2",
    "@visx/scale": "^2.2.2",
    "@visx/shape": "^2.10.0",
    "@visx/tooltip": "^2.10.0",
    "bcrypt": "^5.0.1",
    "canvas": "^2.9.1",
    "chardet": "^1.4.0",
    "classnames": "^2.3.1",
    "colord": "^2.9.2",
    "core-js": "^3.22.7",
    "date-fns": "^2.28.0",
    "debounce": "^1.2.1",
    "downshift": "^6.1.7",
    "email-addresses": "^5.0.0",
    "escape-string-regexp": "^5.0.0",
    "express": "^4.18.1",
    "fast-diff": "^1.2.0",
    "file-saver": "^2.0.5",
    "healthone": "^6.1.0",
    "iban": "^0.0.14",
    "ical-generator": "^3.4.3",
    "iconv-lite": "^0.6.3",
    "idb": "^6.1.5",
    "jspdf": "^2.5.1",
    "lodash.deburr": "^4.1.0",
    "lodash.omit": "^4.5.0",
    "lru-cache": "^7.10.1",
    "medidoc": "1.1.0",
    "meteor-node-stubs": "^1.2.3",
    "notistack": "^2.0.5",
    "papaparse": "^5.3.2",
    "pdfjs-dist": "^2.14.305",
    "qrcode.react": "^3.0.2",
    "rate-limiter-flexible": "^2.3.7",
    "react": "^17.0.2",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.2",
    "react-loading": "^2.0.3",
    "react-number-format": "^4.9.3",
    "react-router-dom": "^6.3.0",
    "react-spring": "^9.4.5",
    "regenerator-runtime": "^0.13.9",
    "sepa-payment-qr-code": "^1.0.0",
    "sharp": "^0.30.5",
    "use-debounce": "^8.0.1",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.18.2",
    "@babel/plugin-syntax-jsx": "7.17.12",
    "@faker-js/faker": "7.1.0",
    "@fontsource/roboto": "4.5.7",
    "@testing-library/dom": "8.13.0",
    "@testing-library/react": "12.1.5",
    "@testing-library/user-event": "14.2.0",
    "@types/chai": "4.3.1",
    "@types/express": "4.17.13",
    "@types/lru-cache": "7.10.9",
    "@types/luxon": "2.3.2",
    "@types/meteor": "2.0.4",
    "@types/mocha": "8.2.0",
    "@types/node": "14.18.3",
    "@types/react": "17.0.45",
    "@types/react-color": "3.0.6",
    "@types/react-dom": "17.0.17",
    "@types/react-router-dom": "5.3.3",
    "@types/sharp": "0.30.2",
    "babel-plugin-istanbul": "6.1.1",
    "babel-plugin-transform-remove-console": "6.9.4",
    "chai": "4.3.6",
    "dayjs": "1.11.2",
    "emoji-favicon-cli": "3.0.1",
    "eslint-config-xo-react": "0.27.0",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "4.5.0",
    "husky": "8.0.1",
    "lint-staged": "12.4.2",
    "moment": "2.29.3",
    "moment-timezone": "0.5.34",
    "mup": "1.5.9",
    "npm-check-updates": "13.0.3",
    "postinstall": "0.7.4",
    "puppeteer": "14.1.1",
    "rrule": "2.6.9",
    "simpl-schema": "1.12.1",
    "total-order": "3.0.1",
    "typescript": "^4.3.5",
    "xo": "0.49.0"
  },
  "postinstall": {
    "@fontsource/roboto/300.css": "copy public/@fontsource/roboto/",
    "@fontsource/roboto/400.css": "copy public/@fontsource/roboto/",
    "@fontsource/roboto/500.css": "copy public/@fontsource/roboto/",
    "@fontsource/roboto/700.css": "copy public/@fontsource/roboto/",
    "@fontsource/roboto/files/*.woff": "copy public/@fontsource/roboto/files/",
    "@fontsource/roboto/files/*.woff2": "copy public/@fontsource/roboto/files/",
    "pdfjs-dist/build/pdf.worker.min.js": "copy public/pdfjs-dist/build/",
    "pdfjs-dist/build/pdf.worker.js.map": "copy public/pdfjs-dist/build/",
    "pdfjs-dist/cmaps/": "copy public/pdfjs-dist/cmaps",
    "pdfjs-dist/standard_fonts/": "copy public/pdfjs-dist/standard_fonts"
  },
  "private": true,
  "scripts": {
    "favicon": "emoji-favicon-cli -d public -v twitter face_with_thermometer",
    "lint": "xo",
    "lint-and-fix": "npm run lint -- --fix",
    "install-hooks": "husky install",
    "postinstall": "postinstall && npm run favicon",
    "precommit": "lint-staged",
    "dev": "meteor run -p ${HOST:-localhost}:${PORT:-12345}",
    "bundle-visualizer": "npm run dev -- --extra-packages bundle-visualizer --production",
    "tsc": "tsc",
    "tsc:watch": "npm run tsc -- --noErrorTruncation --watch",
    "test": "TZ=Europe/Brussels meteor test -p ${HOST:-localhost}:${PORT:-12348} --driver-package meteortesting:mocha",
    "test:dev:non-interactive": "PUPPETEER_EXECUTABLE_PATH=/usr/bin/chromium TEST_BROWSER_DRIVER=puppeteer npm run test --",
    "cover": "BABEL_ENV=COVERAGE COVERAGE=1 COVERAGE_APP_FOLDER=$PWD/ npm run test -- --extra-packages lmieulet:meteor-coverage",
    "cover:dev:non-interactive": "PUPPETEER_EXECUTABLE_PATH=/usr/bin/chromium TEST_BROWSER_DRIVER=puppeteer COVERAGE_OUT_HTML=1 npm run cover --",
    "ci:test": "TEST_BROWSER_DRIVER=puppeteer COVERAGE_OUT_LCOVONLY=1 npm run cover -- --once",
    "upgrade-deps": "meteor node .upgrade/upgrade.js",
    "upgrade": "meteor update && npm run upgrade-deps -- -u && meteor npm install",
    "setup-deploy": "mup setup --config=.deploy/${DEPLOY_SITE:-default}/mup.js --settings=.deploy/${DEPLOY_SITE:-default}/settings.json",
    "build-and-upload": "mup deploy --config=.deploy/${DEPLOY_SITE:-default}/mup.js --settings=.deploy/${DEPLOY_SITE:-default}/settings.json",
    "deploy": "git diff --exit-code && git diff --cached --exit-code && git pull && git checkout $TAG && meteor npm clean-install && npm run build-and-upload"
  },
  "xo": {
    "parser": "@babel/eslint-parser",
    "extends": "xo-react",
    "prettier": true,
    "rules": {
      "camelcase": "off",
      "no-constant-condition": "off",
      "no-negated-condition": "off",
      "capitalized-comments": "off",
      "no-labels": "off",
      "no-warning-comments": "off",
      "import/extensions": "off",
      "import/no-cycle": "warn",
      "node/file-extension-in-import": "off",
      "import/no-anonymous-default-export": "off",
      "unicorn/filename-case": "off",
      "unicorn/no-array-for-each": "off",
      "unicorn/no-array-callback-reference": "off",
      "unicorn/no-array-method-this-argument": "off",
      "unicorn/no-fn-reference-in-iterator": "off",
      "unicorn/prevent-abbreviations": "off",
      "unicorn/prefer-node-protocol": "off",
      "unicorn/prefer-json-parse-buffer": "off",
      "react/function-component-definition": "off",
      "react/state-in-constructor": "off",
      "react/static-property-placement": "off",
      "react/prop-types": "off",
      "react/boolean-prop-naming": "off",
      "react/no-array-index-key": "off",
      "react/no-unsafe": "off",
      "react/require-default-props": "off",
      "react/jsx-no-duplicate-props": [
        "error",
        {
          "ignoreCase": false
        }
      ],
      "react/jsx-curly-brace-presence": [
        "error",
        {
          "props": "never",
          "children": "never",
          "propElementValues": "always"
        }
      ],
      "@typescript-eslint/naming-convention": "off",
      "@typescript-eslint/consistent-type-assertions": "off",
      "@typescript-eslint/no-unsafe-argument": "off",
      "@typescript-eslint/no-unsafe-call": "off",
      "@typescript-eslint/no-unsafe-return": "off",
      "@typescript-eslint/no-unsafe-assignment": "off",
      "@typescript-eslint/no-unsafe-member-access": "off",
      "@typescript-eslint/ban-types": "off",
      "@typescript-eslint/restrict-template-expressions": "off"
    },
    "overrides": [
      {
        "files": [
          "imports/ui/Router.tsx"
        ],
        "rules": {
          "react/no-children-prop": "off"
        }
      },
      {
        "files": [
          "**/client/**/*.{js,jsx,ts,tsx}",
          "imports/ui/**/*.{js,jsx,ts,tsx}"
        ],
        "env": "browser"
      },
      {
        "files": [
          "imports/api/**/*.mock.{js,jsx,ts,tsx}",
          "imports/**/*.tests.{js,jsx,ts,tsx}",
          "imports/test/**/*.{js,jsx,ts,tsx}",
          "test/**/*.app-tests.{js,jsx,ts,tsx}"
        ],
        "env": [
          "meteor",
          "mocha"
        ],
        "rules": {
          "unicorn/prefer-module": "off"
        }
      },
      {
        "files": [
          ".deploy/*/mup.js",
          "lint-staged.config.js"
        ],
        "rules": {
          "unicorn/prefer-module": "off",
          "unicorn/numeric-separators-style": "off"
        }
      }
    ]
  }
}
