{
  "name": "patients",
  "meteor": {
    "mainModule": {
      "client": "client/main.tsx",
      "server": "server/main.ts"
    }
  },
  "babel": {
    "plugins": [
      "@babel/plugin-syntax-jsx"
    ],
    "env": {
      "COVERAGE": {
        "sourceMaps": "both",
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.15.4",
    "@combinatorics/n-combinations": "^0.1.1",
    "@functional-abstraction/operator": "^1.0.4",
    "@heap-data-structure/pairing-heap": "^3.0.1",
    "@iterable-iterator/cardinality": "^3.0.4",
    "@iterable-iterator/chain": "^1.0.0",
    "@iterable-iterator/filter": "^0.1.2",
    "@iterable-iterator/group": "^0.0.1",
    "@iterable-iterator/iter": "^0.0.2",
    "@iterable-iterator/list": "^0.0.2",
    "@iterable-iterator/map": "^0.1.0",
    "@iterable-iterator/next": "^1.0.1",
    "@iterable-iterator/range": "^1.0.0",
    "@iterable-iterator/reduce": "^0.0.1",
    "@iterable-iterator/slice": "^0.0.1",
    "@iterable-iterator/sorted": "^0.0.2",
    "@iterable-iterator/window": "^0.0.3",
    "@iterable-iterator/zip": "^0.0.2",
    "@material-ui/core": "4.11.3",
    "@material-ui/icons": "4.11.2",
    "@material-ui/lab": "4.0.0-alpha.57",
    "@material-ui/pickers": "4.0.0-alpha.10",
    "@set-theory/cartesian-product": "^1.0.2",
    "@total-order/date": "^0.0.2",
    "@total-order/iter": "^0.0.2",
    "@total-order/key": "^0.0.1",
    "@total-order/primitive": "^0.0.2",
    "@visx/axis": "^2.1.2",
    "@visx/event": "^2.1.2",
    "@visx/gradient": "^2.1.0",
    "@visx/grid": "^2.1.2",
    "@visx/group": "^2.1.0",
    "@visx/legend": "^2.1.2",
    "@visx/mock-data": "^2.1.2",
    "@visx/scale": "^2.1.2",
    "@visx/shape": "^2.1.2",
    "@visx/tooltip": "^2.2.0",
    "bcrypt": "^5.0.1",
    "chardet": "^1.4.0",
    "classnames": "^2.3.1",
    "colord": "^2.9.1",
    "date-fns": "^2.25.0",
    "debounce": "^1.2.1",
    "downshift": "^6.1.7",
    "escape-string-regexp": "^5.0.0",
    "express": "^4.17.1",
    "fast-diff": "^1.2.0",
    "file-saver": "^2.0.5",
    "healthone": "^4.0.7",
    "iban": "^0.0.14",
    "ical-generator": "^3.0.1",
    "iconv-lite": "^0.6.3",
    "keycode": "^2.2.0",
    "lodash.deburr": "^4.1.0",
    "meteor-node-stubs": "^1.1.0",
    "notistack": "^1.0.10",
    "papaparse": "^5.3.1",
    "pdfjs-dist": "^2.10.377",
    "prop-types": "^15.7.2",
    "qrcode.react": "^1.0.1",
    "rate-limiter-flexible": "^2.3.2",
    "react": "^17.0.2",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.2",
    "react-loading": "^2.0.3",
    "react-number-format": "^4.7.3",
    "react-router": "^6.0.0",
    "react-router-dom": "^5.3.0",
    "react-spring": "^9.3.0",
    "regenerator-runtime": "^0.13.9",
    "sepa-payment-qr-code": "^1.0.0",
    "use-debounce": "^7.0.0",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.16.0",
    "@babel/plugin-syntax-jsx": "7.16.0",
    "@fontsource/roboto": "4.5.1",
    "@types/express": "4.17.13",
    "@types/luxon": "2.0.5",
    "@types/meteor": "1.4.84",
    "@types/mocha": "9.0.0",
    "@types/node": "16.11.6",
    "@types/react-router-dom": "5.3.2",
    "babel-plugin-istanbul": "6.1.1",
    "chai": "4.3.4",
    "dayjs": "1.10.7",
    "emoji-favicon-cli": "3.0.1",
    "eslint-config-xo-react": "0.25.0",
    "eslint-plugin-react": "7.26.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "faker": "5.5.3",
    "husky": "7.0.4",
    "lint-staged": "11.2.6",
    "moment": "2.29.1",
    "moment-timezone": "0.5.33",
    "mup": "1.5.5",
    "npm-check-updates": "11.8.5",
    "postinstall": "0.7.4",
    "puppeteer": "10.4.0",
    "rrule": "2.6.8",
    "simpl-schema": "1.12.0",
    "total-order": "3.0.1",
    "typescript": "4.4.4",
    "xo": "0.45.0"
  },
  "postinstall": {
    "@fontsource/roboto/300.css": "copy public/@fontsource/roboto/",
    "@fontsource/roboto/400.css": "copy public/@fontsource/roboto/",
    "@fontsource/roboto/500.css": "copy public/@fontsource/roboto/",
    "@fontsource/roboto/700.css": "copy public/@fontsource/roboto/",
    "@fontsource/roboto/files/*.woff": "copy public/@fontsource/roboto/files/",
    "@fontsource/roboto/files/*.woff2": "copy public/@fontsource/roboto/files/",
    "pdfjs-dist/build/pdf.worker.js": "copy public/pdfjs-dist/build/",
    "pdfjs-dist/build/pdf.worker.js.map": "copy public/pdfjs-dist/build/",
    "pdfjs-dist/cmaps/": "copy public/pdfjs-dist/cmaps"
  },
  "private": true,
  "scripts": {
    "favicon": "emoji-favicon-cli -d public -v twitter face_with_thermometer",
    "lint": "xo",
    "lint-and-fix": "npm run lint -- --fix",
    "install-hooks": "husky install",
    "postinstall": "postinstall && npm run favicon",
    "precommit": "lint-staged",
    "dev": "meteor run -p ${HOST:-localhost}:${PORT:-12345}",
    "bundle-visualizer": "npm run dev -- --extra-packages bundle-visualizer --production",
    "tsc": "tsc",
    "tsc:watch": "npm run tsc -- --noErrorTruncation --watch",
    "test": "meteor test -p ${HOST:-localhost}:${PORT:-12348} --driver-package meteortesting:mocha",
    "test:dev:non-interactive": "PUPPETEER_EXECUTABLE_PATH=/usr/bin/chromium TEST_BROWSER_DRIVER=puppeteer npm run test",
    "cover": "BABEL_ENV=COVERAGE COVERAGE=1 COVERAGE_APP_FOLDER=$PWD/ COVERAGE_OUT_LCOVONLY=1 npm run test -- --extra-packages lmieulet:meteor-coverage",
    "ci:test": "TEST_BROWSER_DRIVER=puppeteer npm run cover -- --once",
    "upgrade-deps": "ncu --upgrade",
    "upgrade": "meteor update && npm run upgrade-deps && meteor npm install",
    "setup-deploy": "mup setup --config=.deploy/${DEPLOY_SITE:-default}/mup.js --settings=.deploy/${DEPLOY_SITE:-default}/settings.json",
    "build-and-upload": "mup deploy --config=.deploy/${DEPLOY_SITE:-default}/mup.js --settings=.deploy/${DEPLOY_SITE:-default}/settings.json",
    "deploy": "git diff --exit-code && git diff --cached --exit-code && git pull && git checkout $TAG && meteor npm clean-install && npm run build-and-upload"
  },
  "xo": {
    "parser": "@babel/eslint-parser",
    "extends": "xo-react",
    "prettier": true,
    "rules": {
      "camelcase": "off",
      "no-constant-condition": "off",
      "no-negated-condition": "off",
      "capitalized-comments": "off",
      "no-labels": "off",
      "no-warning-comments": "off",
      "import/extensions": "off",
      "import/no-cycle": "warn",
      "node/file-extension-in-import": "off",
      "import/no-anonymous-default-export": "off",
      "unicorn/filename-case": "off",
      "unicorn/no-array-for-each": "off",
      "unicorn/no-array-callback-reference": "off",
      "unicorn/no-array-method-this-argument": "off",
      "unicorn/no-fn-reference-in-iterator": "off",
      "unicorn/prevent-abbreviations": "off",
      "unicorn/prefer-node-protocol": "off",
      "react/function-component-definition": "off",
      "react/state-in-constructor": "off",
      "react/static-property-placement": "off",
      "react/prop-types": "off",
      "react/boolean-prop-naming": "off",
      "react/no-array-index-key": "off",
      "react/no-unsafe": "off",
      "react/require-default-props": "off",
      "react/jsx-no-duplicate-props": [
        "error",
        {
          "ignoreCase": false
        }
      ],
      "@typescript-eslint/naming-convention": "off",
      "@typescript-eslint/consistent-type-assertions": "off",
      "@typescript-eslint/no-unsafe-argument": "off",
      "@typescript-eslint/no-unsafe-call": "off",
      "@typescript-eslint/no-unsafe-return": "off",
      "@typescript-eslint/no-unsafe-assignment": "off",
      "@typescript-eslint/no-unsafe-member-access": "off",
      "@typescript-eslint/no-floating-promises": "off",
      "@typescript-eslint/ban-types": "off",
      "@typescript-eslint/restrict-template-expressions": "off"
    },
    "overrides": [
      {
        "files": [
          "client/*.js",
          "imports/ui/**/*.js",
          "imports/client/*.js"
        ],
        "env": "browser"
      },
      {
        "files": [
          "imports/api/**/*.mock.js",
          "imports/api/**/*.tests.js"
        ],
        "env": [
          "meteor",
          "mocha"
        ],
        "globals": [
          "Factory"
        ]
      },
      {
        "files": [
          ".deploy/*/mup.js",
          "lint-staged.config.js"
        ],
        "rules": {
          "unicorn/prefer-module": "off",
          "unicorn/numeric-separators-style": "off"
        }
      }
    ]
  }
}
