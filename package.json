{
  "name": "patients",
  "meteor": {
    "mainModule": {
      "client": "client/main.tsx",
      "server": "server/main.ts"
    }
  },
  "babel": {
    "plugins": [
      "@babel/plugin-syntax-jsx"
    ],
    "env": {
      "COVERAGE": {
        "sourceMaps": "both",
        "plugins": [
          [
            "transform-remove-console",
            {
              "exclude": [
                "time",
                "timeEnd",
                "log",
                "error",
                "warn"
              ]
            }
          ],
          "istanbul"
        ]
      }
    }
  },
